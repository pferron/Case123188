    "ts-node": "^10.8.0",
    "tsconfig-paths": "3.9.0"
  },
  "engines": {
    "node": ">=18"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "UNLICENSED",
  "lint-staged": {
    "*.ts": [
      "organize-imports-cli",
      "eslint --cache --fix"
    ]
  },
  "private": true,
  "scripts": {
    "audit": "better-npm-audit audit --production --level high",
    "build": "nest build",
    "build:watch": "tsc -w",
    "console:dev": "ts-node -r tsconfig-paths/register --files src/console.ts",
    "console": "node ./dist/src/console.js",
    "db:seed:fake-data": "npm install && node dist/src/seeds/fake-data/insert-fake-data.js",
    "db:seed:phx-bff-data": "npm install && node dist/src/seeds/fake-data/phx-bff-data.js",
    "db:seed:hrs-data": "npm install && node dist/src/seeds/fake-data/insert-hrs-data.js",
    "hg:sandbox:token": "ts-node ./bin/health-gorilla-sandbox-token.ts",
    "kafka:publish:message": "npx ts-node ./bin/publish-kafka-msg-cli.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "postbuild": "copyfiles --error public/**/* dist",
    "prebuild": "rimraf dist",
    "prestart": "npm run build",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest --config jest.config.js",
    "test:cov": "jest --config jest.config.js --coverage --maxWorkers=2",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --detectOpenHandles",
    "test:e2e": "jest --config ./src/e2e/jest-e2e.json --runInBand --forceExit",
    "test:e2e:cov": "jest --config ./src/e2e/jest-e2e.json --runInBand --coverage --forceExit",
    "test:integration-legacy": "jest --config ./src/integration-legacy/jest-integration.json --runInBand --passWithNoTests --forceExit",
    "test:watch": "jest --coverage --watch",
    "typeorm": "./node_modules/typeorm/cli.js",
    "typeorm:drop-database": "npm run typeorm schema:drop -- -d dist/src/modules/database/config/hrs-data-source.js",
    "typeorm:create": "npm run typeorm migration:create ./src/config/db/migrations/$npm_config_name",
    "typeorm:migrate": "npm run typeorm -- migration:run -d ./dist/src/modules/database/config/hrs-data-source.js",
    "typeorm:migrate:member-alerts": "npm run typeorm -- migration:run -d ./dist/src/modules/database/config/member-alerts-data-source.js",
    "typeorm:migrate:targeted-interventions": "npm run typeorm -- migration:run -d ./dist/src/modules/database/config/targeted-interventions-data-source.js",
    "typeorm:migrate:targeted-interventions-rollups": "npm run typeorm -- migration:run -d ./dist/src/modules/database/config/ti-rollups-data-source.js",
    "typeorm:migrate:cards": "npm run typeorm -- migration:run -d ./dist/src/modules/database/config/cards-data-source.js",
    "typeorm:migrate:all": "npm run typeorm:migrate && npm run typeorm:migrate:member-alerts && npm run typeorm:migrate:targeted-interventions && npm run typeorm:migrate:cards",
    "typeorm:revert": "npm run typeorm -- migration:revert -d ./dist/src/modules/database/config/hrs-data-source.js",
    "typeorm:revert:member-alerts": "npm run typeorm -- migration:revert -d ./dist/src/modules/database/config/member-alerts-data-source.js",
    "typeorm:revert:targeted-interventions": "npm run typeorm -- migration:revert -d ./dist/src/modules/database/config/targeted-interventions-data-source.js",
    "typeorm:revert:targeted-interventions-rollups": "npm run typeorm -- migration:revert -d ./dist/src/modules/database/config/ti-rollups-data-source.js",
    "typeorm:revert:cards": "npm run typeorm -- migration:revert -d ./dist/src/modules/database/config/cards-data-source.js"
  }
}
